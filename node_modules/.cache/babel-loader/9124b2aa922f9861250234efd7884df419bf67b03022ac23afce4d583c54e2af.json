nu by default */\n.navbar-links {\n  opacity: 0;\n  pointer-events: none;\n  transition: opacity 0.3s ease;\n}\n\n/* Show menu on click */\n.navbar-menu.show {\n  opacity: 1;\n  pointer-events: auto;\n}\n</style>\n\n"],"mappings":"AAqBA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACF,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;IAChC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}